  <link rel="stylesheet" type="text/css" href="../../assets/css/keen-dashboards.css" />

</head>
<body class="application">

<script type="text/javascript" charset="utf-8">

    $(function(){
        new Highcharts.Chart({
            chart: {
                renderTo: "ejemplo"
            },
            title: {
                text: " Pedidos emitidos a grupos"},
            xAxis: {
                categories: ['Grupo 3', 'Grupo 4', 'Grupo 5', 'Grupo 6','Grupo 7']
            },
            yAxis: {
                title:{
                    text: "cantidad"
                }
            },
            series: [{
                name: "leche",
                data: [1,2,3,4,5]


            },
                {
                    name: "cacao",
                    data: [1,4,3,4,2]

                },
                {
                    name: "huevo",
                    data: [1,1,5,3,4]

                },
                {
                    name: "semola",
                    data: [3,4,3,3,3]

                },
                {
                    name: "sal",
                    data: [3,1,1,1,1]


                }]





        });
        new Highcharts.Chart({
            chart: {
                renderTo: "ejemplo2"
            },
            title: {
                text: "Pedidos recibidos de grupos"},
            xAxis: {
               categories: ['Grupo 1','Grupo 2','Grupo 3','Grupo 6']
            },
            yAxis: {
                title:{
                    text: "Dollars"
                }
            },
            series: [{
                name: "azucar",
                data: [1,2,3,4]
            },
                {
                    name: "celulosa",
                    data:[0,3,0,0]

                }]
        });
    });

</script>

<h1 style="margin-top: -30px"> Reporte de pedido a grupos</h1>
<hr>
