  <div class="row">
    <div class="col-sm-10">
      <div class="chart-wrapper">

        <div class="chart-stage">
          <div id="ejemplo"></div>
        </div>
      </div>
    </div>

    <div class="col-sm-10">
      <div class="chart-wrapper">
        <div class="chart-stage">
          <div id="ejemplo2"></div>
        </div>

      </div>
    </div>

  </div>



